{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "bill": {
        "ClusterId": "powerbillingusage-api",
        "Match": {
          "Path": "/api/bill/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "powerbillingusage-api": {
        "Destinations": {
          "bill": {
            "Address": "https://localhost:7046"
          },
          "bill-two": {
            "Address": "https://localhost:7046"
          }
        },
        "LoadBalancingPolicy": "RoundRobin"
      }
    }
  }
}
